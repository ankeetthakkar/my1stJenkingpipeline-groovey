pipeline
  {
    agent any
	     stages
		 {
		    stage('clone')
			{
			   step{
                    sh 'echo clone'			      
				   }
			}
			
			stage('test cases')
			{
			    step{
				
				   sh 'echo test cases'
				    }
			}
			
			stage('build')
			{
			    step{
				    sh 'echo build'
				    }
			
			}
	     }
		 
		 
		 
		 
		 
	}
  
